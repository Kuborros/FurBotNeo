  </target>
  
  <target name="clean" depends="clean.module.furbotneo" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.furbotneo" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
