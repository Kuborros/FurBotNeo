<configuration debug="false">

    <!--STDOUT-->
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{HH:mm:ss}] [%boldBlue(%level)] [%boldGreen(%logger{0})]: %msg%n</pattern>
        </encoder>
    </appender>


    <!--Root logger-->
    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
